<UserControl x:Class="TripleTriad.View.ViewUsuarioRanking"
             xmlns:viewBase="clr-namespace:BaseView;assembly=BaseView"
             xmlns:controles="clr-namespace:TripleTriad.View.Controles"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006">

    <Grid>
        <viewBase:ViewConsultaBase>
            <viewBase:ViewConsultaBase.Filtros>
                <controles:UsuarioRankingFiltros/>
            </viewBase:ViewConsultaBase.Filtros>
            
            <viewBase:ViewConsultaBase.Detalhe>
                <controles:UsuarioRankingDetalhe/>
            </viewBase:ViewConsultaBase.Detalhe>
            
            <viewBase:ViewConsultaBase.ConsultaItem>
                <DataTemplate>
                    <controles:UsuarioRankingConsultaItem/>
                </DataTemplate>
            </viewBase:ViewConsultaBase.ConsultaItem>

        </viewBase:ViewConsultaBase>
    </Grid>
</UserControl>
